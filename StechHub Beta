--// LockAim
local LockAimSection = AimTab:AddSection("Aim Lock")
local LockAimToggle = AimTab:AddToggle(
    "LockAimToggle",
    {
        Title = "Enabled",
        Default = false,
        Callback = LockAimFunc
    }
)
local LockAimKeybind = AimTab:AddKeybind(
    "LockAimKeybind",
    {
        Title = "Keybind",
        Mode = "Toggle",
        Default = "Y",
        Callback = function()
            LockAimToggle:SetValue(not AimLock.Enabled)
            if AimLock.Enabled then
                TurnOn.Playing = true
            else
                TurnOff.Playing = true
            end
            Notify("Lock Aim", EnabledDisabled[tostring(AimLock.Enabled)])
        end
    }
)
